{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/wall/nether_brick_wall_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "block/wall/nether_brick_wall_side" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/wall/nether_brick_wall_side", "y": 90 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/wall/nether_brick_wall_side", "y": 180 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/wall/nether_brick_wall_side", "y": 270 }
        }
    ]
}
